  <新版函件收文>
    <StepName>函件管理员处理</StepName>
    <Role>函件管理员</Role>
  </新版函件收文>
  <党纪工团发文>
    <StepName>分发</StepName>
    <Role>党群文书组</Role>
  </党纪工团发文>
  <党纪工团收文>
    <StepName>收文处理中心归档</StepName>
    <Role>党群文书组</Role>
  </党纪工团收文>
  <程序文件>
    <StepName>分发</StepName>
    <Role>程序管理员</Role>
  </程序文件>
  <请示报告>
    <StepName>承办审核</StepName>
    <Role>公司办秘书</Role>
  </请示报告>
